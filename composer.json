{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "ext-zlib": "*",
        "aws/aws-sdk-php": "^3.198",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/folio": "^1.1",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^4.0",
        "laravel/octane": "^2.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "laravel/vapor-cli": "^1.27",
        "laravel/vapor-core": "^2.33",
        "laravel/vapor-ui": "^1.3",
        "livewire/livewire": "^3.0",
        "openai-php/laravel": "^0.8.0",
        "puklipo/laravel-vapor-gzip": "^1.0",
        "revolution/laravel-line-sdk": "^3.1",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-sitemap": "^6.1",
        "torann/json-ld": "^0.0.19"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "itsgoingd/clockwork": "^5.1",
        "laravel-lang/common": "^5.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vapor-ui:publish --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sail:up": [
            "./vendor/bin/sail up -d",
            "./vendor/bin/sail art ide-helper:models -M"
        ],
        "sail:down": "./vendor/bin/sail down"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-sockets": "1.0.0"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
